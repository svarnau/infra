[ircbot]
nick=<%= nick %>
pass=<%= password %>
server=<%= server %>
port=6667
channels=<%= channels %>
nicks=<%= auth_nicks %>
lockfile=/var/run/statusbot/statusbot.pid
log_config=/etc/statusbot/logging.config

[wiki]
username=<%= wiki_user %>
password=<%= wiki_password %>
url=<%= wiki_url %>
pageid=<%= wiki_pageid %>

[alertfile]
dir=/var/lib/statusbot/www
